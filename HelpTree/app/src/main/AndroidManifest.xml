<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dicoding.picodiploma.helptree">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HelpTree">
        <activity
            android:name=".ui.chatbot.ChatbotActivity"
            android:label="@string/title_activity_chatbot"
            android:theme="@style/ChatbotTheme" />
        <activity android:name=".ui.picture.TakePictureActivity" />
        <activity android:name=".ui.home.WebViewAct" />
        <activity android:name=".ui.information.InfoActivity" />
        <activity
            android:name=".ui.home.SplashScreen"
            android:theme="@style/SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.home.HomeActivity"/>
    </application>

</manifest>